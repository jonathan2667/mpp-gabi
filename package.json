{
    "name": "mpp-exam-template",
    "version": "1.0.0",
    "description": "Full-stack application template based on mppEx1 structure",
    "main": "index.js",
    "scripts": {
        "start": "concurrently \"npm run backend\" \"npm run frontend\"",
        "backend": "cd backend && npm run dev",
        "frontend": "cd frontend && npm run dev",
        "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install",
        "dev": "npm start",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [
        "fullstack",
        "express",
        "vanilla-js",
        "sqlite"
    ],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "concurrently": "^8.2.2"
    }
}